<Window x:Class="DataBase_Course_Work.AddNewProtocol"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBase_Course_Work"
        mc:Ignorable="d"
        Title="Добавить протокол" Height="350" Width="300">
    <Window.Resources>
        <ResourceDictionary Source="Style.xaml"/>
    </Window.Resources>
    <StackPanel Orientation="Vertical">
        <Label>Показания свидетелей</Label>
        <TextBox Height="50" x:Name="TextBoxWitnessReadings"/>
        <Label>Показания потерпевшего</Label>
        <TextBox Height="50" x:Name="TextBoxPlaintiffReadings"/>
        <Label>Показания подозреваемого</Label>
        <TextBox Height="50" x:Name="TextBoxDefendantReadings"/>
        <Button Content="Добавить" Click="ButtonBase_OnClick"/>
    </StackPanel>
</Window>
